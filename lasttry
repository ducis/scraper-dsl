$`[_-$a@x,_+$b@y,_`find`$c@z,_+$+//@+//@]
ExLeftRec (ExRef "")
  (LrrPostfix []
     (OpComposed '[' ','
        [ExLeftRec ExSlot
           (LrrInfix (OpSymbolic "-") []
              [ExNamed (ExRef "a") (Name '@' Nothing "x" Nothing)]),
         ExLeftRec ExSlot
           (LrrInfix (OpSymbolic "+") []
              [ExNamed (ExRef "b") (Name '@' Nothing "y" Nothing)]),
         ExLeftRec ExSlot
           (LrrInfix (OpAlphabetic "find") []
              [ExNamed (ExRef "c") (Name '@' Nothing "z" Nothing)]),
         ExLeftRec
           (ExLeftRec
              (ExLeftRec ExSlot (LrrInfix (OpSymbolic "+") [] [ExRef ""]))
              (LrrInfix (OpSymbolic "+") []
                 [ExNamed (ExSelector '/' "") (Name '@' Nothing "" Nothing)]))
           (LrrInfix (OpSymbolic "+") []
              [ExNamed (ExSelector '/' "") (Name '@' Nothing "" Nothing)])])
     [])
$`[_-$a@x,_+$b@y,_`find`$c@z,_+$+//@+//@]
T1
  (AApplication [T1 (ARef "") [AFBindings []]]
     (AOMany
        (AMSimple
           [T1
              (AApplication
                 [T1 ASlot [AFBindings []],
                  T1 (ABind (T1 (ARef "a") [AFBindings []]) "x")
                    [AFBindings ["a", "x"]]]
                 (AOSym "-"))
              [AFBindings []],
            T1
              (AApplication
                 [T1 ASlot [AFBindings []],
                  T1 (ABind (T1 (ARef "b") [AFBindings []]) "y")
                    [AFBindings ["a", "y"]]]
                 (AOSym "+"))
              [AFBindings []],
            T1
              (AApplication
                 [T1 ASlot [AFBindings []],
                  T1 (ABind (T1 (ARef "c") [AFBindings []]) "z")
                    [AFBindings ["a", "z"]]]
                 (AOAlpha "find"))
              [AFBindings []],
            T1
              (AApplication
                 [T1
                    (AApplication
                       [T1
                          (AApplication
                             [T1 ASlot [AFBindings []], T1 (ARef "") [AFBindings []]]
                             (AOSym "+"))
                          [AFBindings []],
                        T1 (ABind (T1 (ASelector "") [AFBindings []]) "")
                          [AFBindings ["", "a"]]]
                       (AOSym "+"))
                    [AFBindings []],
                  T1 (ABind (T1 (ASelector "") [AFBindings []]) "")
                    [AFBindings ["", "a"]]]
                 (AOSym "+"))
              [AFBindings []]])))
  [AFBindings []]
